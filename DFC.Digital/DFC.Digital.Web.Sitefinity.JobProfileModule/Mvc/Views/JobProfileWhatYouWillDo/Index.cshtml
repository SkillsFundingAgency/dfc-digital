@model DFC.Digital.Web.Sitefinity.JobProfileModule.Mvc.Models.JobProfileWhatYouWillDoViewModel
<section class="job-profile-subsection" id="@Html.Raw(Model.SectionId)">
    <h2 class="heading-large">@Html.Raw(Model.Title)</h2>
    <h3>@Model.WhatyouWillDoSectionTitle</h3>
    <div class="job-profile-subsection-content">
        @Html.Raw(Model.TopSectionContent)
        @Html.Raw(Model.PropertyValue)
        @Html.Raw(Model.BottomSectionContent)
    </div>
</section>

@if (Model.IsWhatYouWillDoCadView)
{
    @Html.Partial("Workingenvironments", Model)
}