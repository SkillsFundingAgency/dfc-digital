@model DFC.Digital.Web.Sitefinity.Widgets.Mvc.Models.DfcBreadcrumbViewModel

<div class="content-container">
    <div class="breadcrumbs">
        <ol>
            <li><a href="@Model.HomepageLink">@Model.HomepageText</a></li>
            @foreach (var item in Model.BreadcrumbLinks)
            {
                <li><a href="@item.Link">@item.Text</a></li>
            }
        </ol>
    </div>
</div>