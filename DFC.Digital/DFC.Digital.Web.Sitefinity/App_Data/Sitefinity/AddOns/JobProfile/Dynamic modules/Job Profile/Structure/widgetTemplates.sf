    "Theme": null,
    "DataType": "ASP_NET_TEMPLATE",
    "Data": "@model Telerik.Sitefinity.Frontend.Mvc.Models.ContentDetailsViewModel\n\n@using System.Collections;\n@using System.Linq;\n@using Telerik.Sitefinity.Frontend.DynamicContent.WidgetTemplates.Fields.Helpers;\n@using Telerik.Sitefinity;\n@using Telerik.Sitefinity.Data.ContentLinks;\n@using Telerik.Sitefinity.Frontend.Mvc.Helpers;\n@using Telerik.Sitefinity.Libraries.Model;\n@using Telerik.Sitefinity.Localization;\n@using Telerik.Sitefinity.Model;\n@using Telerik.Sitefinity.Model.ContentLinks;\n@using Telerik.Sitefinity.Modules.Pages;\n@using Telerik.Sitefinity.Pages.Model;\n\n<div class=\"@Model.CssClass\" @Html.InlineEditingAttributes(Model.ProviderName, Model.ContentType.FullName, (Guid)Model.Item.Fields.Id)>\n    <h3 @Html.InlineEditingFieldAttributes(\"Title\", \"ShortText\")>\n        @Model.Item.Fields.Title\n    </h3>\n    <div>\n        @Model.Item.GetDateTime(\"PublicationDate\", \"MMM d, yyyy, HH:mm tt\")\n        @Html.CommentsCount(\"\", @Model.Item.DataItem)\n    </div>\n    \n    \n@*Start AlternativeTitle field*@\n<div>\n    <strong> Alternative Title :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"AlternativeTitle\", \"ShortText\")>@Model.Item.Fields.AlternativeTitle</span>    \n</div>\n@*End AlternativeTitle field*@\r\n\n@*Start Overview field*@\n<div>\n    <strong> Overview :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"Overview\", \"ShortText\")>@Model.Item.Fields.Overview</span>    \n</div>\n@*End Overview field*@\r\n\n@*Start SalaryRange field*@\n<div>\n    <strong> Salary Range :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"SalaryRange\", \"ShortText\")>@Model.Item.Fields.SalaryRange</span>    \n</div>\n@*End SalaryRange field*@\r\n\n@*Start CourseKeywords field*@\n<div>\n    <strong> Course keywords :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"CourseKeywords\", \"ShortText\")>@Model.Item.Fields.CourseKeywords</span>    \n</div>\n@*End CourseKeywords field*@\r\n\n@*Start BAUSystemOverrideUrl field*@\n<div>\n    <strong> BAU system override url :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"BAUSystemOverrideUrl\", \"ShortText\")>@Model.Item.Fields.BAUSystemOverrideUrl</span>    \n</div>\n@*End BAUSystemOverrideUrl field*@\r\n\n@*Start IsLMISalaryFeedOverriden field*@\n<div>\n    <strong> Is LMI Salary Feed Overriden :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"IsLMISalaryFeedOverriden\", \"YesNo\")>\n        @Model.Item.GetBool(\"IsLMISalaryFeedOverriden\")\n    </span> \n</div>\n@*End IsLMISalaryFeedOverriden field*@\r\n\n@*Start DoesNotExistInBAU field*@\n<div>\n    <strong> Does not exist in BAU :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"DoesNotExistInBAU\", \"YesNo\")>\n        @Model.Item.GetBool(\"DoesNotExistInBAU\")\n    </span> \n</div>\n@*End DoesNotExistInBAU field*@\r\n\n@*Start SalaryStarter field*@\n<div>\n    <strong> Salary Starter :</strong>\n\n    @if(Model.Item.Fields.SalaryStarter!=null)\n    {\n    <span>@Model.Item.Fields.SalaryStarter</span>   \n    <span></span>\n    }\n</div>\n@*End SalaryStarter field*@\r\n\n@*Start SalaryExperienced field*@\n<div>\n    <strong> Salary Experienced :</strong>\n\n    @if(Model.Item.Fields.SalaryExperienced!=null)\n    {\n    <span>@Model.Item.Fields.SalaryExperienced</span>   \n    <span></span>\n    }\n</div>\n@*End SalaryExperienced field*@\r\n\n@*Start MinimumHours field*@\n<div>\n    <strong> Minimum Hours (mandatory) :</strong>\n\n    @if(Model.Item.Fields.MinimumHours!=null)\n    {\n    <span>@Model.Item.Fields.MinimumHours</span>   \n    <span></span>\n    }\n</div>\n@*End MinimumHours field*@\r\n\n@*Start MaximumHours field*@\n<div>\n    <strong> Maximum Hours (mandatory) :</strong>\n\n    @if(Model.Item.Fields.MaximumHours!=null)\n    {\n    <span>@Model.Item.Fields.MaximumHours</span>   \n    <span></span>\n    }\n</div>\n@*End MaximumHours field*@\r\n\n@*Start Salary field*@\n<div @Html.InlineEditingFieldAttributes(\"Salary\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.Salary)</div>\n@*End Salary field*@\r\n\n@*Start HowToBecome field*@\n<div @Html.InlineEditingFieldAttributes(\"HowToBecome\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.HowToBecome)</div>\n@*End HowToBecome field*@\r\n\n@*Start Skills field*@\n<div @Html.InlineEditingFieldAttributes(\"Skills\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.Skills)</div>\n@*End Skills field*@\r\n\n@*Start WhatYouWillDo field*@\n<div @Html.InlineEditingFieldAttributes(\"WhatYouWillDo\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.WhatYouWillDo)</div>\n@*End WhatYouWillDo field*@\r\n\n@*Start WorkingHoursPatternsAndEnvironment field*@\n<div @Html.InlineEditingFieldAttributes(\"WorkingHoursPatternsAndEnvironment\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.WorkingHoursPatternsAndEnvironment)</div>\n@*End WorkingHoursPatternsAndEnvironment field*@\r\n\n@*Start CareerPathAndProgression field*@\n<div @Html.InlineEditingFieldAttributes(\"CareerPathAndProgression\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.CareerPathAndProgression)</div>\n@*End CareerPathAndProgression field*@\r\n\n@*Start HiddenAlternativeTitle field*@\n<div>\n    <strong> Hidden Alternative Title :</strong>\n\n    @if(Model.Item.Fields.HiddenAlternativeTitle!=null)\n    {\n    <ul @Html.InlineEditingFieldAttributes(\"HiddenAlternativeTitle\", \"FlatTaxon\")>\n    @foreach (var taxon in Model.Item.GetFlatTaxons(\"HiddenAlternativeTitle\"))\n    {\n        <li>@taxon.Title.ToString()</li>   \n    }\n    </ul>\n    }\n</div>\n@*End HiddenAlternativeTitle field*@\r\n\n@*Start JobProfileSpecialism field*@\n<div>\n    <strong> Job Profile Specialism :</strong>\n\n    @if(Model.Item.Fields.JobProfileSpecialism!=null)\n    {\n    <ul @Html.InlineEditingFieldAttributes(\"JobProfileSpecialism\", \"FlatTaxon\")>\n    @foreach (var taxon in Model.Item.GetFlatTaxons(\"JobProfileSpecialism\"))\n    {\n        <li>@taxon.Title.ToString()</li>   \n    }\n    </ul>\n    }\n</div>\n@*End JobProfileSpecialism field*@\r\n\n@*Start WorkingHoursDetails field*@\n<div>\n    <strong> Working Hours Detail (mandatory) :</strong>\n\n    @if(Model.Item.Fields.WorkingHoursDetails!=null)\n    {\n    <span @Html.InlineEditingFieldAttributes(\"WorkingHoursDetails\", \"FlatTaxon\" )>\n        @if (Model.Item.GetFlatTaxon(\"WorkingHoursDetails\") != null)\n        {\n        @Model.Item.GetFlatTaxon(\"WorkingHoursDetails\").Title.ToString();\n        }\n    </span>\n    }\n</div>\n@*End WorkingHoursDetails field*@\r\n\n@*Start WorkingPattern field*@\n<div>\n    <strong> Working Pattern (mandatory) :</strong>\n\n    @if(Model.Item.Fields.WorkingPattern!=null)\n    {\n    <span @Html.InlineEditingFieldAttributes(\"WorkingPattern\", \"FlatTaxon\" )>\n        @if (Model.Item.GetFlatTaxon(\"WorkingPattern\") != null)\n        {\n        @Model.Item.GetFlatTaxon(\"WorkingPattern\").Title.ToString();\n        }\n    </span>\n    }\n</div>\n@*End WorkingPattern field*@\r\n\n@*Start WorkingPatternDetails field*@\n<div>\n    <strong> Working Pattern Detail :</strong>\n\n    @if(Model.Item.Fields.WorkingPatternDetails!=null)\n    {\n    <span @Html.InlineEditingFieldAttributes(\"WorkingPatternDetails\", \"FlatTaxon\" )>\n        @if (Model.Item.GetFlatTaxon(\"WorkingPatternDetails\") != null)\n        {\n        @Model.Item.GetFlatTaxon(\"WorkingPatternDetails\").Title.ToString();\n        }\n    </span>\n    }\n</div>\n@*End WorkingPatternDetails field*@\r\n\n@*Start JobProfileCategories field*@\n<div>\n    <strong> Job Profile Categories :</strong>\n\n    @if(Model.Item.Fields.JobProfileCategories!=null)\n    {\n    <ul @Html.InlineEditingFieldAttributes(\"JobProfileCategories\", \"HierarchicalTaxon\")>\n     @foreach (var taxon in Model.Item.GetHierarchicalTaxons(\"JobProfileCategories\"))\n    {\n        <li>@taxon.Title.ToString()</li>   \n    }\n    </ul>\n    }\n</div>\n@*End JobProfileCategories field*@\r\n\n@*Start SOC field*@\n<div>\n    <strong> Related job profile socs :</strong>\n    \n    @if(Model.Item.Fields.SOC != null)\n    {\n        <a href='@Model.Item.Fields.SOC.DefaultUrl'>\n            @Model.Item.Fields.SOC.Identifier\n        </a>\n    }\n</div>\n@*End SOC field*@\r\n\n@*Start RelatedCareerProfiles field*@\n<div>\n    <strong> Related career profiles</strong>\n\n    <ul>\n    @foreach (var relatedItem in Model.Item.Fields.RelatedCareerProfiles)\n    {\n        <li>\n            <a href='@relatedItem.DefaultUrl'>\n                @relatedItem.Identifier\n            </a>\n        </li>   \n    }\n    </ul>\n</div>\n@*End RelatedCareerProfiles field*@\r\n\n@*Start RelatedInterests field*@\n<div>\n    <strong> Related interests</strong>\n\n    <ul>\n    @foreach (var relatedItem in Model.Item.Fields.RelatedInterests)\n    {\n        <li>\n            <a href='@relatedItem.DefaultUrl'>\n                @relatedItem.Identifier\n            </a>\n        </li>   \n    }\n    </ul>\n</div>\n@*End RelatedInterests field*@\r\n\n@*Start RelatedEntryQualifications field*@\n<div>\n    <strong> Related entry qualifications</strong>\n\n    <ul>\n    @foreach (var relatedItem in Model.Item.Fields.RelatedEntryQualifications)\n    {\n        <li>\n            <a href='@relatedItem.DefaultUrl'>\n                @relatedItem.Identifier\n            </a>\n        </li>   \n    }\n    </ul>\n</div>\n@*End RelatedEntryQualifications field*@\r\n\n@*Start RelatedTrainingRoutes field*@\n<div>\n    <strong> Related training routes</strong>\n\n    <ul>\n    @foreach (var relatedItem in Model.Item.Fields.RelatedTrainingRoutes)\n    {\n        <li>\n            <a href='@relatedItem.DefaultUrl'>\n                @relatedItem.Identifier\n            </a>\n        </li>   \n    }\n    </ul>\n</div>\n@*End RelatedTrainingRoutes field*@\r\n\n@*Start RelatedEnablers field*@\n<div>\n    <strong> Related enablers</strong>\n\n    <ul>\n    @foreach (var relatedItem in Model.Item.Fields.RelatedEnablers)\n    {\n        <li>\n            <a href='@relatedItem.DefaultUrl'>\n                @relatedItem.Identifier\n            </a>\n        </li>   \n    }\n    </ul>\n</div>\n@*End RelatedEnablers field*@\r\n\n@*Start RelatedJobAreas field*@\n<div>\n    <strong> Related job areas</strong>\n\n    <ul>\n    @foreach (var relatedItem in Model.Item.Fields.RelatedJobAreas)\n    {\n        <li>\n            <a href='@relatedItem.DefaultUrl'>\n                @relatedItem.Identifier\n            </a>\n        </li>   \n    }\n    </ul>\n</div>\n@*End RelatedJobAreas field*@\r\n\n@*Start RelatedPreferredTaskTypes field*@\n<div>\n    <strong> Related preferred task types</strong>\n\n    <ul>\n    @foreach (var relatedItem in Model.Item.Fields.RelatedPreferredTaskTypes)\n    {\n        <li>\n            <a href='@relatedItem.DefaultUrl'>\n                @relatedItem.Identifier\n            </a>\n        </li>   \n    }\n    </ul>\n</div>\n@*End RelatedPreferredTaskTypes field*@\r\n    \n\n    @{        \n        if (Model.EnableSocialSharing)\n        {\n            var item = Model.Item.DataItem as Telerik.Sitefinity.Model.IHasTitle;\n            @Html.SocialShareOptions(item);\n        }\n    }\n\n    @Html.CommentsList(@Model.Item.DataItem)\n</div>",
