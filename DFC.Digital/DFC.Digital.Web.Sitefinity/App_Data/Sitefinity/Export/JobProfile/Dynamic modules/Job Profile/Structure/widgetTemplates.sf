mentsAverageRatingControl runat=\"server\" ThreadType='<%# Container.DataItem.GetType().FullName%>' ThreadKey='<%# ControlUtilities.GetLocalizedKey(Eval(\"Id\"), null, CommentsBehaviorUtilities.GetLocalizedKeySuffix(Container.DataItem.GetType().FullName)) %>' NavigateUrl=\"#commentsWidget\" DisplayMode=\"FullText\" />\r\n            <sf:SitefinityLabel runat=\"server\" Text='<%# ControlUtilities.Sanitize(Eval(\"Description\")) %>' WrapperTagName=\"div\" HideIfNoText=\"true\" CssClass=\"sfitemLongText\" data-sf-field=\"Description\" data-sf-ftype=\"LongText\" />\r\n            <div class='sfitemShortTxtWrp'>        \r\n                <sf:SitefinityLabel runat=\"server\" Text='ONet Occupational Code:' WrapperTagName=\"div\" HideIfNoText=\"true\" CssClass=\"sfitemFieldLbl\" />        \r\n                <sf:SitefinityLabel runat=\"server\" Text='<%#: Eval(\"ONetOccupationalCode\") %>' WrapperTagName=\"div\" HideIfNoText=\"true\" CssClass=\"sfitemShortTxt\" data-sf-field=\"ONetOccupationalCode\" data-sf-ftype=\"ShortText\"/>\r\n            </div>\r\n            <div class='sfitemFlatTaxon sfitemTaxonWrp' data-sf-field='apprenticeshipstandards' data-sf-ftype='FlatTaxon'>        \r\n                <sf:SitefinityLabel runat=\"server\" Text='Apprenticeship Standards:' WrapperTagName=\"div\" HideIfNoText=\"true\" CssClass=\"sfitemFieldLbl\" />\r\n                <sf:FlatTaxonField runat=\"server\" DisplayMode=\"Read\" WebServiceUrl=\"~/Sitefinity/Services/Taxonomies/FlatTaxon.svc\" TaxonomyId=\"f16125b6-e9b6-42bb-b116-7969bb1e9126\" AllowMultipleSelection=\"true\" TaxonomyMetafieldName=\"apprenticeshipstandards\" Expanded=\"false\" BindOnServer=\"true\" LayoutTemplatePath=\"~/SFRes/Telerik.Sitefinity.Resources.Templates.Fields.FlatTaxonFieldReadMode.ascx\" />\r\n            </div>\r\n            <div class='sfitemFlatTaxon sfitemTaxonWrp' data-sf-field='apprenticeshipframeworks' data-sf-ftype='FlatTaxon'>        \r\n                <sf:SitefinityLabel runat=\"server\" Text