    "Data": "@model Telerik.Sitefinity.Frontend.Mvc.Models.ContentDetailsViewModel\n\n@using System.Collections;\n@using System.Linq;\n@using Telerik.Sitefinity.Frontend.DynamicContent.WidgetTemplates.Fields.Helpers;\n@using Telerik.Sitefinity;\n@using Telerik.Sitefinity.Data.ContentLinks;\n@using Telerik.Sitefinity.Frontend.Mvc.Helpers;\n@using Telerik.Sitefinity.Libraries.Model;\n@using Telerik.Sitefinity.Localization;\n@using Telerik.Sitefinity.Model;\n@using Telerik.Sitefinity.Model.ContentLinks;\n@using Telerik.Sitefinity.Modules.Pages;\n@using Telerik.Sitefinity.Pages.Model;\n\n<div class=\"@Model.CssClass\" @Html.InlineEditingAttributes(Model.ProviderName, Model.ContentType.FullName, (Guid)Model.Item.Fields.Id)>\n    <h3 @Html.InlineEditingFieldAttributes(\"Title\", \"ShortText\")>\n        @Model.Item.Fields.Title\n    </h3>\n    <div>\n        @Model.Item.GetDateTime(\"PublicationDate\", \"MMM d, yyyy, HH:mm tt\")\n        @Html.CommentsCount(\"\", @Model.Item.DataItem)\n    </div>\n    \n    \n@*Start Title field*@\n<div>\n    <strong> Job Title :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"Title\", \"ShortText\")>@Model.Item.Fields.Title</span>    \n</div>\n@*End Title field*@\r\n\n@*Start AlternativeTitle field*@\n<div>\n    <strong> Alternative Title :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"AlternativeTitle\", \"ShortText\")>@Model.Item.Fields.AlternativeTitle</span>    \n</div>\n@*End AlternativeTitle field*@\r\n\n@*Start WidgetContentTitle field*@\n<div>\n    <strong> Widget content title :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"WidgetContentTitle\", \"ShortText\")>@Model.Item.Fields.WidgetContentTitle</span>    \n</div>\n@*End WidgetContentTitle field*@\r\n\n@*Start Overview field*@\n<div>\n    <strong> Overview :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"Overview\", \"ShortText\")>@Model.Item.Fields.Overview</span>    \n</div>\n@*End Overview field*@\r\n\n@*Start SalaryRange field*@\n<div>\n    <strong> Salary Range :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"SalaryRange\", \"ShortText\")>@Model.Item.Fields.SalaryRange</span>    \n</div>\n@*End SalaryRange field*@\r\n\n@*Start CourseKeywords field*@\n<div>\n    <strong> Course keywords :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"CourseKeywords\", \"ShortText\")>@Model.Item.Fields.CourseKeywords</span>    \n</div>\n@*End CourseKeywords field*@\r\n\n@*Start BAUSystemOverrideUrl field*@\n<div>\n    <strong> BAU system override url :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"BAUSystemOverrideUrl\", \"ShortText\")>@Model.Item.Fields.BAUSystemOverrideUrl</span>    \n</div>\n@*End BAUSystemOverrideUrl field*@\r\n\n@*Start DynamicTitlePrefix field*@\n<div>\n    <strong> Dynamic Title Prefix :</strong>\n\n    @if(Model.Item.Fields.DynamicTitlePrefix != null)\n    {\n        <span @Html.InlineEditingFieldAttributes(\"DynamicTitlePrefix\", \"ChoicesRadioButtons\")>\n            @Model.Item.GetChoiceLabel(\"DynamicTitlePrefix\", @Model.ContentType)\n        </span>\n    }\n</div>\n@*End DynamicTitlePrefix field*@\r\n\n@*Start DigitalSkillsLevel field*@\n<div>\n    <strong> Select Digital Skills Level :</strong>\n\n    @if(Model.Item.Fields.DigitalSkillsLevel != null)\n    {\n        <span @Html.InlineEditingFieldAttributes(\"DigitalSkillsLevel\", \"ChoicesRadioButtons\")>\n            @Model.Item.GetChoiceLabel(\"DigitalSkillsLevel\", @Model.ContentType)\n        </span>\n    }\n</div>\n@*End DigitalSkillsLevel field*@\r\n\n@*Start IsLMISalaryFeedOverriden field*@\n<div>\n    <strong> Is LMI Salary Feed Overriden :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"IsLMISalaryFeedOverriden\", \"YesNo\")>\n        @Model.Item.GetBool(\"IsLMISalaryFeedOverriden\")\n    </span> \n</div>\n@*End IsLMISalaryFeedOverriden field*@\r\n\n@*Start IsHTBCaDReady field*@\n<div>\n    <strong> Is this JobProfile How To Become Section CaD Ready :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"IsHTBCaDReady\", \"YesNo\")>\n        @Model.Item.GetBool(\"IsHTBCaDReady\")\n    </span> \n</div>\n@*End IsHTBCaDReady field*@\r\n\n@*Start IsWYDCadReady field*@\n<div>\n    <strong> Is WYD CAD ready :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"IsWYDCadReady\", \"YesNo\")>\n        @Model.Item.GetBool(\"IsWYDCadReady\")\n    </span> \n</div>\n@*End IsWYDCadReady field*@\r\n\n@*Start DoesNotExistInBAU field*@\n<div>\n    <strong> Does not exist in BAU :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"DoesNotExistInBAU\", \"YesNo\")>\n        @Model.Item.GetBool(\"DoesNotExistInBAU\")\n    </span> \n</div>\n@*End DoesNotExistInBAU field*@\r\n\n@*Start IsImported field*@\n<div>\n    <strong> Is Imported :</strong>\n\n    <span @Html.InlineEditingFieldAttributes(\"IsImported\", \"YesNo\")>\n        @Model.Item.GetBool(\"IsImported\")\n    </span> \n</div>\n@*End IsImported field*@\r\n\n@*Start SalaryStarter field*@\n<div>\n    <strong> Salary Starter :</strong>\n\n    @if(Model.Item.Fields.SalaryStarter!=null)\n    {\n    <span>@Model.Item.Fields.SalaryStarter</span>   \n    <span></span>\n    }\n</div>\n@*End SalaryStarter field*@\r\n\n@*Start SalaryExperienced field*@\n<div>\n    <strong> Salary Experienced :</strong>\n\n    @if(Model.Item.Fields.SalaryExperienced!=null)\n    {\n    <span>@Model.Item.Fields.SalaryExperienced</span>   \n    <span></span>\n    }\n</div>\n@*End SalaryExperienced field*@\r\n\n@*Start MinimumHours field*@\n<div>\n    <strong> Minimum Hours (mandatory) :</strong>\n\n    @if(Model.Item.Fields.MinimumHours!=null)\n    {\n    <span>@Model.Item.Fields.MinimumHours</span>   \n    <span></span>\n    }\n</div>\n@*End MinimumHours field*@\r\n\n@*Start MaximumHours field*@\n<div>\n    <strong> Maximum Hours (mandatory) :</strong>\n\n    @if(Model.Item.Fields.MaximumHours!=null)\n    {\n    <span>@Model.Item.Fields.MaximumHours</span>   \n    <span></span>\n    }\n</div>\n@*End MaximumHours field*@\r\n\n@*Start EntryRoutes field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"EntryRoutes\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.EntryRoutes)</div>\n@*End EntryRoutes field*@\r\n\n@*Start UniversityRelevantSubjects field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"UniversityRelevantSubjects\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.UniversityRelevantSubjects)</div>\n@*End UniversityRelevantSubjects field*@\r\n\n@*Start UniversityFurtherRouteInfo field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"UniversityFurtherRouteInfo\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.UniversityFurtherRouteInfo)</div>\n@*End UniversityFurtherRouteInfo field*@\r\n\n@*Start CollegeRelevantSubjects field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"CollegeRelevantSubjects\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.CollegeRelevantSubjects)</div>\n@*End CollegeRelevantSubjects field*@\r\n\n@*Start CollegeFurtherRouteInfo field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"CollegeFurtherRouteInfo\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.CollegeFurtherRouteInfo)</div>\n@*End CollegeFurtherRouteInfo field*@\r\n\n@*Start ApprenticeshipRelevantSubjects field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"ApprenticeshipRelevantSubjects\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.ApprenticeshipRelevantSubjects)</div>\n@*End ApprenticeshipRelevantSubjects field*@\r\n\n@*Start ApprenticeshipFurtherRouteInfo field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"ApprenticeshipFurtherRouteInfo\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.ApprenticeshipFurtherRouteInfo)</div>\n@*End ApprenticeshipFurtherRouteInfo field*@\r\n\n@*Start Work field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"Work\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.Work)</div>\n@*End Work field*@\r\n\n@*Start Volunteering field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"Volunteering\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.Volunteering)</div>\n@*End Volunteering field*@\r\n\n@*Start DirectApplication field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"DirectApplication\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.DirectApplication)</div>\n@*End DirectApplication field*@\r\n\n@*Start OtherRoutes field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"OtherRoutes\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.OtherRoutes)</div>\n@*End OtherRoutes field*@\r\n\n@*Start OtherRequirements field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"OtherRequirements\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.OtherRequirements)</div>\n@*End OtherRequirements field*@\r\n\n@*Start CareerTips field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"CareerTips\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.CareerTips)</div>\n@*End CareerTips field*@\r\n\n@*Start ProfessionalAndIndustryBodies field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"ProfessionalAndIndustryBodies\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.ProfessionalAndIndustryBodies)</div>\n@*End ProfessionalAndIndustryBodies field*@\r\n\n@*Start FurtherInformation field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"FurtherInformation\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.FurtherInformation)</div>\n@*End FurtherInformation field*@\r\n\n@*Start HowToBecome field*@\n<div class=\"sf-Long-text\" @Html.InlineEditingFieldAttributes(\"HowToBecome\", \"LongText\")>@Html.HtmlSanitize((string)Model.Item.Fields.HowToBecome)</div>\n@*End HowToB