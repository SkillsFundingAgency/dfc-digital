<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>JobProfile</id>
    <version>2.0.31.5</version>
    <title>JobProfile</title>
    <authors>DFC development team</authors>
    <owners>DFC</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>https://nuget.org/Content/Images/packageDefaultIcon-50x50.png</iconUrl>
    <description>
      Job Profile Dynamic Module -- Updating Skills with additional PSF fields.
      This nuget package contains additional fields needed for Skills Dynamic Content that are required for searching JobProfiles based on PSF filters.
    </description>
    <language>en-GB</language>
    <references></references>
  </metadata>
  <files>
    <file src="\**\*.*" exclude="\**\*.ps1" target="content\App_Data\Sitefinity\AddOns\JobProfile" />
    <file src="\**\*.ps1" target="tools" />
    <file src="**\*.psm1" target="tools" />
  </files>
</package>