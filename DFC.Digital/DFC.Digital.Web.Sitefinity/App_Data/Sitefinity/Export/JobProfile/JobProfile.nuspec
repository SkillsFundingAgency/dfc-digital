<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>JobProfile</id>
    <version>2.0.31.6</version>
    <title>JobProfile</title>
    <authors>DFC development team</authors>
    <owners>DFC</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>https://nuget.org/Content/Images/packageDefaultIcon-50x50.png</iconUrl>
    <description>
      Job Profile Dynamic Module -- Adding PSF related fields to SKills Dynamic Content
      This nuget package contains all the new PSF fields added to the Skills Dynamic Content Type.
      These new fields and classification were added to search JobProfiles based on PSF data.
    </description>
    <language>en-GB</language>
  </metadata>
  <files>
    <file src="**\*.*" exclude="**\*.ps1" target="content\App_Data\Sitefinity\AddOns\JobProfile" />
    <file src="**\*.ps1" target="tools" />
    <file src="\**\*.psm1" target="tools" />
  </files>
</package>