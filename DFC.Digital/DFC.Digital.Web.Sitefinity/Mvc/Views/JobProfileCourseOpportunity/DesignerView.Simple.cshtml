    <li class="sfFormSeparator" style="padding-bottom: 15px;">

        <label for="prop-DefaultJobProfileUrlName">Default JobProfile UrlName</label>
        <div>
            <input class="form-control" ng-model="properties.DefaultJobProfileUrlName.PropertyValue" type="text" />
