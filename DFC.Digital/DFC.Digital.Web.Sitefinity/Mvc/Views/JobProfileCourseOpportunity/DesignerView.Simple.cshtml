    <li class="sfFormSeparator" style="padding-bottom: 15px;">

        <label for="prop-CourseLink">Course Link</label>
        <div>
            <input class="form-control" ng-model="properties.CourseLink.PropertyValue" type="text" />
        </div>
    </li>
    <li class="sfFormSeparator" style="padding-bottom: 15px;">

        <label for="prop-CurrentJobProfileUrl">Current JobProfile Url</label>
        <div>
            <input class="form-control" ng-model="properties.CurrentJobProfileUrl.PropertyValue" type="text" />
