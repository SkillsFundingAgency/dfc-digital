<ul class="sfForm">
    <li class="sfFormSeparator" style="padding-bottom: 15px;">

        <label for="prop-MainSectionTitle">Main Section Title</label>
        <div>
            <input class="form-control" ng-model="properties.MainSectionTitle.PropertyValue" type="text" />
        </div>
    </li>
    <li class="sfFormSeparator" style="padding-bottom: 15px;">

        <label for="prop-SectionId">Section Id</label>
        <div>
            <input class="form-control" ng-model="properties.SectionId.PropertyValue" type="text" />
        </div>
    </li>
    <li class="sfFormSeparator" style="padding-bottom: 15px;">

        <label for="prop-CoursesSectionTitle">Courses Section Title</label>
        <div>
            <input class="form-control" ng-model="properties.CoursesSectionTitle.PropertyValue" type="text" />
        </div>
    </li>
    <li class="sfFormSeparator" style="padding-bottom: 15px;">

        <label for="prop-TrainingCoursesLocationDetails">Training Courses Location Details</label>
        <div>
            <input class="form-control" ng-model="properties.TrainingCoursesLocationDetails.PropertyValue" type="text" />
        </div>
