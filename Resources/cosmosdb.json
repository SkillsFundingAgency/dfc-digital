{
	"DatabaseName": "CourseSearch",
	"Collections": [
    {
      "CollectionName": "AuditRecords ",
      "OfferThroughput": 500,
      "IndexingPolicy": {
        "indexingMode": "consistent",
        "automatic": true,
        "includedPaths": [
          {
            "path": "/*",
            "indexes": [
              {
                "kind": "Range",
                "dataType": "Number",
                "precision": -1
              },
              {
                "kind": "Range",
                "dataType": "String",
                "precision": -1
              }
            ]
          }
        ],
        "excludedPaths": []
      }
    }
	]
}